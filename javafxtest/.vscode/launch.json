{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Debug (Launch) - Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-App<javafxtest>",
      "request": "launch",
      "mainClass": "app.App",
      "projectName": "javafxtest",
      "vmArgs": "--module-path $PATH_TO_FX --add-modules javafx.controls"
    }
  ]
}
